            "content": [
                {
                    "type": "paragraph",
                    "content": [
                        {
                            "type": "text",
                            "text": "Im "
                        },
                        {
                            "type": "text",
                            "text": "IssueService",
                            "marks": [
                                {
                                    "type": "code"
                                }
                            ]
                        },
                        {
                            "type": "text",
                            "text": " gibt es schon eine Methode, um von "
                        },
                        {
                            "type": "text",
                            "text": "$json",
                            "marks": [
                                {
                                    "type": "code"
                                }
                            ]
                        },
                        {
                            "type": "text",
                            "text": "  ein Issue zu mappen."
                        }
                    ]
                },
                {
                    "type": "paragraph",
                    "content": []
                },
                {
                    "type": "codeBlock",
                    "attrs": {},
                    "content": [
                        {
                            "type": "text",
                            "text": "class IssueService extends \\JiraRestApi\\JiraClient\n{\n    private $uri = '/issue';\n\n    /**\n     * @param object $json\n     *\n     * @throws \\JsonMapper_Exception\n     *\n     * @return Issue\n     */\n    public function getIssueFromJSON($json): Issue\n    {\n        $issue = $this->json_mapper->map(\n            $json,\n            new Issue()\n        );\n\n        return $issue;\n    }"
                        }
                    ]
                }
            ]
        },
